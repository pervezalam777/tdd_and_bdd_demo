"src/model/appModel.js","original":{"line":95,"column":28}},{"generated":{"line":118,"column":22},"source":"src/model/appModel.js","original":{"line":95,"column":31}},{"name":"create","generated":{"line":119,"column":0},"source":"src/model/appModel.js","original":{"line":96,"column":2}},{"name":"create","generated":{"line":119,"column":2},"source":"src/model/appModel.js","original":{"line":96,"column":2}},{"generated":{"line":119,"column":8},"source":"src/model/appModel.js","original":{"line":96,"column":8}},{"generated":{"line":119,"column":10},"source":"src/model/appModel.js","original":{"line":96,"column":10}},{"generated":{"line":120,"column":0},"source":"src/model/appModel.js","original":{"line":96,"column":10}},{"generated":{"line":120,"column":11},"source":"src/model/appModel.js","original":{"line":96,"column":16}},{"name":"AppModel","generated":{"line":120,"column":15},"source":"src/model/appModel.js","original":{"line":96,"column":20}},{"generated":{"line":120,"column":23},"source":"src/model/appModel.js","original":{"line":96,"column":16}},{"generated":{"line":120,"column":25},"source":"src/model/appModel.js","original":{"line":96,"column":10}},{"generated":{"line":121,"column":0},"source":"src/model/appModel.js","original":{"line":96,"column":10}},{"generated":{"line":122,"column":0},"source":"src/model/appModel.js","original":{"line":95,"column":31}},{"generated":{"line":122,"column":1},"source":"src/model/appModel.js","original":{"line":95,"column":7}}],"sources":{"src/model/appModel.js":"//@ts-nocheck\r\n\r\n//NOTE: Application Model also behave like application level event dispatcher\r\n// Of course there are other ways to create application level dispatcher.\r\n// that makes event driven application. As it is demo purpose so keeping application\r\n// level dispatcher role here in this module only. It will be supplied to all the screens as and when required.\r\n\r\n/**\r\n * interface AppEvent {\r\n *  type:string\r\n *  payload?:any\r\n * }\r\n * \r\n * interface Dispatcher {\r\n *  registerListener(forEvent:string, listenerFunction:Function);\r\n *  removeListener(forEvent:string, listenerFunction:Function);\r\n *  dispatchEvent(event:AppEvent)\r\n * }\r\n */\r\nimport { LOGIN_SUCCESS } from '../constants/eventConst.js'\r\n\r\nclass AppModel {\r\n  _userDetails = null \r\n  _listeners = {}\r\n\r\n  constructor(){}\r\n\r\n  init(){\r\n    // The following scenario is normally used in less secure application.\r\n    // TODO: User has logged in and\r\n    // Refresh the browser\r\n    // if user token cookie available \r\n    // if user re-opens the application while user token cookie is still valid. \r\n\r\n    //In case more secure application session will time out as soon as\r\n    // user leave application idle for 60 second (value may vary base on requirement)\r\n    // user close the tab\r\n    // user refresh the screen.\r\n  }\r\n\r\n  registerListener(forEvent, listenerFunction){\r\n    if(!this._listeners[forEvent]){\r\n      this._listeners[forEvent] = [listenerFunction];\r\n      return true;\r\n    }\r\n    const listenerList = this._listeners[forEvent];\r\n    if(!listenerList.includes(listenerFunction)){\r\n      listenerList.push(listenerFunction);\r\n      return true;\r\n    }\r\n    return false; \r\n  }\r\n\r\n  removeListener(forEvent, listenerFunction){\r\n    const listeners = this._listeners[forEvent];\r\n    if(listeners && listeners.length > 0 && listeners.includes(listenerFunction)){\r\n      this._listeners[forEvent] = listeners.filter(fn => fn !== listenerFunction);\r\n      return true;\r\n    }\r\n    return false;\r\n  }\r\n\r\n  dispatchEvent(event){\r\n    const listeners = this._listeners[event.type];\r\n    if(listeners && listeners.length > 0){\r\n      listeners.forEach(fn => {\r\n        fn.apply(null, event);\r\n      })\r\n    }\r\n  }\r\n\r\n  set userDetails(data){\r\n    this._userDetails = data;\r\n    setTimeout(() => {\r\n      this.dispatchEvent({type:LOGIN_SUCCESS});\r\n    }, 0)\r\n  }\r\n  \r\n  get userName() { \r\n    return this._userDetails.username;\r\n  }\r\n  get userToken() {\r\n    return this._userDetails.token;\r\n  }\r\n \r\n  get userEmail(){\r\n    return this._userDetails.email;\r\n  }\r\n\r\n  get userRole(){\r\n    return this._userDetails.role;\r\n  }\r\n}\r\n\r\nexport const AppModelFactory = {\r\n  create: () => new AppModel()\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"2bab3f24d5840fc33a5e08444c624940","cacheData":{"env":{}}}